        <xs:documentation>&lt;p&gt;Signature Method Algorithm for binary signature&lt;/p&gt;
&lt;p&gt;Accepted Values:&lt;/p&gt;
&lt;p&gt;MD2WithRSAEncryption&lt;/p&gt;
&lt;p&gt;MD4WithRSAEncryption&lt;/p&gt;
&lt;p&gt;MD5WithRSAEncryption&lt;/p&gt;
&lt;p&gt;RIPEMD128WithRSAEncryption&lt;/p&gt;
&lt;p&gt;RIPEMD160WithRSAEncryption&lt;/p&gt;
&lt;p&gt;RIPEMD256WithRSAEncryption&lt;/p&gt;
&lt;p&gt;SHA1WithRSAEncryption&lt;/p&gt;
&lt;p&gt;SHA224WithRSAEncryption&lt;/p&gt;
&lt;p&gt;SHA256WithRSAEncryption&lt;/p&gt;
&lt;p&gt;SHA512WithRSAEncryption&lt;/p&gt;
&lt;p&gt;HmacSHA1&lt;/p&gt;
&lt;p&gt;HmacSHA256&lt;/p&gt;
&lt;p&gt;HmacSHA384&lt;/p&gt;
&lt;p&gt;HmacSHA512&lt;/p&gt;
&lt;p&gt;HmacMD5&lt;/p&gt;
&lt;p&gt;Default: HmacSHA1&lt;/p&gt;</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="xs:string" use="optional" name="key">
