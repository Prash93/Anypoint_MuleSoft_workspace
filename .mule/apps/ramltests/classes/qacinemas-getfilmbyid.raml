#%RAML 1.0
title: QA Cinemas
version: 1.0
baseUri: http://apps.qacinemas.com/api/
              
/getFilms:
  /{byFilmID}:
    displayName: Film
    uriParameters: 
      byFilmID:
        displayName: Film ID
        type: integer
    get:
      responses: 
        200:
          body: 
            application/json:
              example: |
                {
                  "filmID": 1,
                  "film": "Wonder Women",
                  "genre": "Action"
                }
  